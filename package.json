{
  "name": "timelapse-front",
  "version": "0.1.0",
  "description": "Minimalist Timelapse",
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "start": "NODE_ENV=production node server.js",
    "test": "./node_modules/.bin/nightwatch --env local",
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-postbuild": "echo This runs afterwards.",
    "sauce": "node nightwatch.conf.js && ./node_modules/.bin/nightwatch -e chrome,chromemac,firefox",
    "postinstall": "NODE_ENV=production webpack -p --progress",
    "dev": "node nightwatch.conf.js && webpack-dev-server --inline --hot --progress",
    "eslint": "eslint src --ignore-path ./.gitignore --ext=.js,.jsx",
    "stylelint": "stylelint ./css/*.scss --config ./stylelint.config.js",
    "lint": "npm run eslint && npm run stylelint",
    "stylefmt": "stylefmt ./css/*.scss -c ./stylelint.config.js",
    "validate": "npm ls"
  },
  "author": "me@jfroffice.me",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.0",
    "node-sass": "^4.3.0",
    "optimist": "^0.6.1",
    "pre-commit": "^1.1.3",
    "precommit-hook": "^3.0.0",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "draft-js": "^0.9.1",
    "express": "^4.14.0",
    "medium-draft": "^0.4.0",
    "moment": "^2.15.2",
    "nightwatch": "^0.9.12",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2",
    "saucelabs": "1.3.0",
    "selenium-download": "2.0.9",
    "string.prototype.startswith": "0.2.0",
    "whatwg-fetch": "2.0.2"
  },
  "pre-commit": [
    "stylelint",
    "lint"
  ]
}
